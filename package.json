{
  "name": "algomatic",
  "version": "3.2.1",
  "description": "Various algorithms and math utilities.",
  "main": "./index.js",
  "module": "./index.mjs",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc --project tsconfig.build.json && rollup --config && cp package.json README.md LICENSE.txt lib/ && cd lib && npm pkg delete scripts devDependencies",
    "clean": "rimraf gen lib docs coverage",
    "test": "jest",
    "perf": "toofast",
    "docs": "typedoc src/main/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smikhalevski/algomatic.git"
  },
  "keywords": [
    "javascript",
    "int",
    "uint",
    "byte",
    "number",
    "math",
    "bitwise",
    "lerp",
    "spline",
    "cspline",
    "cubic-spline",
    "interpolation",
    "binary-search",
    "sort",
    "parallel-sort",
    "easing"
  ],
  "author": "Savva Mikhalevski <smikhalevski@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/smikhalevski/algomatic/issues"
  },
  "homepage": "https://github.com/smikhalevski/algomatic#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@types/jest": "^29.5.4",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "rollup-plugin-dts": "^6.0.1",
    "toofast": "^2.0.0",
    "ts-jest": "^29.1.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typedoc-custom-css": "github:smikhalevski/typedoc-custom-css#master",
    "typescript": "^5.1.6"
  }
}
